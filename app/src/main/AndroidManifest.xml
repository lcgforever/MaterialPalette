<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chenguang.materialpalette">

    <application
        android:name=".MaterialPaletteApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ColorInfoActivity" />
        <activity android:name=".activity.AboutAppActivity" />
        <activity android:name=".activity.CopyrightActivity" />
        <activity android:name=".activity.PaletteActivity" />
        <activity android:name=".activity.PreviewActivity" />
    </application>

</manifest>